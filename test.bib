@article{Lauritzen2016a,
  title = {Rod-Cone Crossover Connectome of Mammalian Bipolar Cells},
  issn = {10969861},
  doi = {10.1002/cne.24084},
  abstract = {\textcopyright{} 2016 Wiley Periodicals, Inc. The basis of cross-suppression between rod and cone channels has long been an enigma. Using rabbit retinal connectome RC1, we show that all cone bipolar cell (BC) classes inhibit rod BCs via amacrine cell (AC) motifs (C1-6); that all cone BC classes are themselves inhibited by AC motifs (R1-5, R25) driven by rod BCs. A sparse symmetric AC motif (CR) is presynaptic and postsynaptic to both rod and cone BCs. ON cone BCs of all classes drive inhibition of rod BCs via motif C1 wide-field GABAergic ACs ($\gamma$ACs) and motif C2 narrow field glycinergic ON ACs (GACs). Each rod BC receives $\approx$10 crossover AC synapses and each ON cone BC can target $\approx$10 or more rod BCs via separate AC processes. OFF cone BCs mediate monosynaptic inhibition of rod BCs via motif C3 driven by OFF $\gamma$ACs and GACs and disynaptic inhibition via motifs C4 and C5 driven by OFF wide-field $\gamma$ACs and narrow-field GACs, respectively. Motifs C4 and C5 form halos of 60-100 inhibitory synapses on proximal dendrites of AI $\gamma$ACs. Rod BCs inhibit surrounding arrays of cone BCs through AII GAC networks that access ON and OFF cone BC patches via motifs R1, R2, R4, R5 and a unique ON AC motif R3 that collects rod BC inputs and targets ON cone BCs. Crossover synapses for motifs C1, C4, C5, and R3 are 3-4\texttimes{} larger than typical feedback synapses, which may be a signature for synaptic winner-take-all switches.},
  journal = {Journal of Comparative Neurology},
  author = {Lauritzen, J.S. and Sigulinsky, C.L. and Anderson, J.R. and Kalloniatis, M. and Nelson, N.T. and Emrich, D.P. and Rapp, C. and Mccarthy, N. and Kerzner, E. and Meyer, M. and Jones, B.W. and Marc, R.E.},
  year = {2016},
  keywords = {Networks,Connectomics,Amacrine cells,Cone vision,Retina,Rod vision,RRID AB_2341093,RRID AB_2532053,RRID AB_2532055,RRID AB_2532057,RRID AB_2532059,RRID AB_2532060,RRID AB_2532061,RRID SCR_001622,RRID SCR_005986,RRID SCR_008394,RRID SCR-002937,RRID SCR-008606,Synapses,Transmission electron microscopy}
}

@inproceedings{Dou2014a,
  title = {Adaptive Depth Bias for Shadow Maps},
