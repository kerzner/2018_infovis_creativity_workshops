@article{Rousselle2011,
  title = {Adaptive Sampling and Reconstruction Using Greedy Error Minimization},
  volume = {30},
  issn = {07300301},
  doi = {10.1145/2070781.2024193},
  abstract = {We introduce a novel approach for image space adaptive sampling and reconstruction in Monte Carlo rendering. We greedily minimize relative mean squared error (MSE) by iterating over two steps. First, given a current sample distribution, we optimize over a discrete set of filters at each pixel and select the filter that minimizes the pixel error. Next, given the current filter selection, we distribute additional samples to further reduce MSE. The success of our approach hinges on a robust technique to select suitable per pixel filters. We develop a novel filter selection procedure that robustly solves this problem even with noisy input data. We evaluate our approach using effects such as motion blur, depth of field, interreflections, etc. We provide a comparison to a state-of-the-art algorithm based on wavelet shrinkage and show that we achieve significant improvements in numerical error and visual image quality. Our approach is simple to implement, requires a single user parameter, and is compatible with standard Monte Carlo rendering. \textcopyright{} 2011 ACM.},
  number = {6},
  journal = {ACM Transactions on Graphics},
  author = {Rousselle, Fabrice and Knaus, Claude and Zwicker, Matthias},
  year = {2011},
